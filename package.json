{
  "author": "Balaji L Narayanan <lbalaji8385@gmail.com>",
  "name": "localtunnel-server",
  "description": "Expose any port to world",
  "version": "1.0.1",
  "license": "MIT",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/balaji8385/localtunnel-server.git"
  },
  "dependencies": {
    "book": "1.3.3",
    "dotenv": "16.4.7",
    "esm": "3.2.25",
    "express": "5.1.0",
    "express-jwt": "8.5.1",
    "http-proxy": "1.18.1",
    "human-id": "4.1.2",
    "localenv": "0.2.2",
    "pump": "3.0.3",
    "tldts": "6.1.85",
    "typescript": "5.9.3",
    "uuid": "11.1.0",
    "winston": "3.18.3"
  },
  "bugs": {
    "url": "https://github.com/balaji8385/localtunnel-server/issues"
  },
  "homepage": "https://github.com/balaji8385/localtunnel-server#readme",
  "keywords": [
    "localtunnel",
    "localtunnel2",
    "triotunnel",
    "triotunnel-server",
    "tunnelling",
    "no-dns",
    "easyhosting"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "auto-changelog": "^2.5.0",
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@babel/register": "^7.28.3",
    "@types/express": "5.0.5",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "22.14.0",
    "@types/optimist": "0.0.33",
    "@types/pump": "1.1.3",
    "@types/winston": "2.4.4",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "mocha": "11.7.5",
    "node-dev": "8.0.0",
    "npm-check-updates": "^17.1.16",
    "supertest": "7.1.4",
    "ws": "8.18.3"
  },
  "scripts": {
    "start": "node dist/index.js",
    "test": "mocha --check-leaks --require 'test/**/Client.test.js'",
    "dev": "ts-node-dev src --port 3000",
    "ncu": "ncu --interactive --no-deprecated --format group",
    "build": "babel src --out-dir dist --extensions .ts,.js && tsc",
    "changelog": "auto-changelog --template ./changelog-template.hbs --output CHANGELOG.md --unreleased",
    "postpublish": "echo 'Published version $npm_package_version'",
    "version": "echo \"Bumping to $npm_package_version\"",
    "postversion": "npm run changelog && git add CHANGELOG.md && git commit -m 'chore: update changelog' && git push && git push --tags || git tag -f v$npm_package_version && git push -f --tags"
  }
}
